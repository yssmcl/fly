	<queryString>
		<![CDATA[SELECT
     laudo.id_laudo AS laudo_id_laudo,
     laudo.resultado AS laudo_resultado,
     laudo.observacao AS laudo_observacao,
     laudo.dt_laudo AS laudo_dt_laudo,
     laudo.indicacao AS laudo_indicacao,
     exame.id_exame AS exame_id_exame,
     exame.dt_exame AS exame_dt_exame,
     profissional.id_profissional AS profissional_id_profissional,
     profissional.nome AS profissional_nome,
     paciente.id_paciente AS paciente_id_paciente,
     paciente.nome AS paciente_nome,
     tipo_exame.id_tipo_exame AS tipo_exame_id_tipo_exame,
     tipo_exame.desc_tipo_exame AS tipo_exame_desc_tipo_exame
FROM
     exame exame INNER JOIN laudo laudo ON exame.id_exame = laudo.id_exame
     INNER JOIN profissional profissional ON profissional.id_profissional = laudo.id_profissional
     INNER JOIN paciente paciente ON exame.id_paciente = paciente.id_paciente
     INNER JOIN tipo_exame tipo_exame ON exame.id_tipo_exame = tipo_exame.id_tipo_exame
WHERE laudo.id_laudo = $P{id_laudo}
]]>
	</queryString>
