<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/protected/MarcaEquipamentoList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{marcaEquipamentoHome.wire}"/>

   <param name="marcaEquipamentoFrom"/>
   <param name="marcaEquipamentoIdMarcaEquipamento" value="#{marcaEquipamentoHome.marcaEquipamentoIdMarcaEquipamento}"/>


   <navigation from-action="#{marcaEquipamentoHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/protected/MarcaEquipamento.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{marcaEquipamentoHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/protected/MarcaEquipamento.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{marcaEquipamentoHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/protected/MarcaEquipamentoList.xhtml"/>
      </rule>
   </navigation>

</page>
