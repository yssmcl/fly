<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/protected/TipoEquipamentoList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{tipoEquipamentoHome.wire}"/>

   <param name="tipoEquipamentoFrom"/>
   <param name="tipoEquipamentoIdTipoEquipamento" value="#{tipoEquipamentoHome.tipoEquipamentoIdTipoEquipamento}"/>


   <navigation from-action="#{tipoEquipamentoHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/protected/TipoEquipamento.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tipoEquipamentoHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/protected/TipoEquipamento.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tipoEquipamentoHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/protected/TipoEquipamentoList.xhtml"/>
      </rule>
   </navigation>

</page>
