<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/protected/TipoExameList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{tipoExameHome.wire}"/>

   <param name="tipoExameFrom"/>
   <param name="tipoExameIdTipoExame" value="#{tipoExameHome.tipoExameIdTipoExame}"/>


   <navigation from-action="#{tipoExameHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/protected/TipoExame.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tipoExameHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/protected/TipoExame.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tipoExameHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/protected/TipoExameList.xhtml"/>
      </rule>
   </navigation>

</page>
