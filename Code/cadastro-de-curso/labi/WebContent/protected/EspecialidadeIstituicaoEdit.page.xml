<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/protected/EspecialidadeIstituicaoList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{especialidadeIstituicaoHome.wire}"/>

   <param name="especialidadeIstituicaoFrom"/>
   <param name="especialidadeIstituicaoIdEspecialidadeIstituicao" value="#{especialidadeIstituicaoHome.especialidadeIstituicaoIdEspecialidadeIstituicao}"/>
   <param name="instituicaoFrom"/>
   <param name="instituicaoIdInstituicao" value="#{instituicaoHome.instituicaoIdInstituicao}"/>
   <param name="cidadeFrom"/>
   <param name="cidadeIdCidade" value="#{cidadeHome.cidadeIdCidade}"/>
   <param name="paisFrom"/>
   <param name="paisIdPais" value="#{paisHome.paisIdPais}"/>





   <navigation from-action="#{especialidadeIstituicaoHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/protected/EspecialidadeIstituicao.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{especialidadeIstituicaoHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/protected/EspecialidadeIstituicao.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{especialidadeIstituicaoHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/protected/EspecialidadeIstituicaoList.xhtml"/>
      </rule>
   </navigation>

</page>
